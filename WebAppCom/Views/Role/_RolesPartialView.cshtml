@model IEnumerable<WebAppCom.Models.Rol>
<div class="table-responsive-md">
    <table class="table settingsTable" style="width:100%">
        <thead>
            <tr>
                <th>
                    Descripción
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Description)
                </td>
                <td class="text-center">
                    <a onclick="loadPartialRol(@item.RolId)"><i class="fa fa-marker fa-lg"></i></a> |

                    <a onclick="deleteItem(@item.RolId, '/Role/RolDelete/')"
                       class="text-danger">
                        <i class="fa fa-trash fa-lg"></i>
                    </a>
                </td>
            </tr>
             }
        </tbody>
    </table>
</div>